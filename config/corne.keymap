#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>


/ {
        behaviors {
                hrm: home_row_mod_left {
                        compatible = "zmk,behavior-hold-tap";
                        #binding-cells = <2>;
                        flavor = "tap-preferred";
                        require-prior-idle-ms = <150>;
                        tapping-term-ms = <200>;
                        bindings = <&kp>, <&kp>;
                        hold-trigger-on-release;
                };

                tkl: thumb_keys_left {
                        compatible = "zmk,behavior-hold-tap";
                        #binding-cells = <2>;
                        flavor = "balanced";
                        require-prior-idle-ms = <150>;
                        tapping-term-ms = <150>;
                        bindings = <&kp>, <&kp>;
                        // Right-hand positions
                        hold-trigger-key-positions = <
                                6 7 8 9 10 11
                                18 19 20 21 22 23
                                30 31 32 33 34 35
                                39 40 41
                        >;
                        hold-trigger-on-release;
                };
                tkr: thumb_keys_right {
                        compatible = "zmk,behavior-hold-tap";
                        #binding-cells = <2>;
                        flavor = "balanced";
                        require-prior-idle-ms = <150>;
                        tapping-term-ms = <150>;
                        bindings = <&kp>, <&kp>;
                        // Left-hand positions
                        hold-trigger-key-positions = <
                                0 1 2 3 4 5
                                12 13 14 15 16 17
                                24 25 26 27 28 29
                                36 37 38
                        >;
                        hold-trigger-on-release;
                };
        };

        keymap {
                compatible = "zmk,keymap";

                default_layer {
// -----------------------------------------------------------------------------------------
// |  TAB |  Q  |  W  |  E  |  R  |  T  |   |  Y  |  U   |  I  |  O  |  P  | BKSP |
// | CTRL |  A  |  S  |  D  |  F  |  G  |   |  H  |  J   |  K  |  L  |  ;  |  '   |
// | SHFT |  Z  |  X  |  C  |  V  |  B  |   |  N  |  M   |  ,  |  .  |  /  | ESC  |
//                    | GUI | LWR | SPC |   | ENT | RSE  | ALT |
                        bindings = <
   &kp TAB   &kp Q       &kp W       &kp E        &kp R        &kp T   &kp Y &kp U        &kp I        &kp O       &kp P           &kp BSPC
   &kp ESC   &hrm LGUI A &hrm LALT S &hrm LSHFT D &hrm LCTRL F &kp G   &kp H &hrm RCTRL J &hrm RSHFT K &hrm RALT L &hrm RCTRL SEMI &kp ENTER
   &kp LSHFT &kp Z       &kp X       &kp C        &kp V        &kp B   &kp N &kp M        &kp COMMA    &kp DOT     &kp FSLH        &kp RSFT
                             &kp ESC &kp SPACE &kp TAB   &kp RET &kp BACKSPACE &kp RALT
                        >;
                };

//                 lafayette_layer {
// // -----------------------------------------------------------------------------------------
// // |     |  ^  |  <  |  >  |  $  |  %  |   |  @  |  &  |  *  |  '  |  `  |      |
// // |     |  {  |  (  |  )  |  }  |  =  |   |  \  |  +  |  -  |  /  |  "  |      |
